{
  "$comment" : "NOTE: This file is for human reference ONLY. For programmatic use, use the GET '/json_schema/query_params/$schema_name' endpoints, or within conch itself, json-schema/query_params.yaml.\nNote that all parameters are parsed internally from the request URI as strings, so all type checks here use strings. When a query parameter is used more than once, its values are parsed as an arrayref. See ../modules/Conch::Plugin::JSONValidator#validate_query_params.",
  "$defs" : {
    "Anything" : {
      "additionalProperties" : true,
      "type" : "object"
    },
    "BuildDevices" : {
      "$ref" : "#/$defs/FindDevice",
      "allOf" : [
        {
          "not" : {
            "properties" : {
              "ids_only" : {
                "const" : "1"
              },
              "serials_only" : {
                "const" : "1"
              }
            },
            "required" : [
              "ids_only",
              "serials_only"
            ],
            "type" : "object"
          }
        },
        {
          "not" : {
            "required" : [
              "phase_earlier_than",
              "phase"
            ],
            "type" : "object"
          }
        }
      ],
      "default" : {
        "ids_only" : "0",
        "serials_only" : "0"
      },
      "properties" : {
        "active_minutes" : {
          "$ref" : "#/$defs/non_negative_integer_string"
        },
        "health" : {
          "oneOf" : [
            {
              "$ref" : "common.json#/$defs/device_health"
            },
            {
              "items" : {
                "$ref" : "common.json#/$defs/device_health"
              },
              "minItems" : 2,
              "type" : "array",
              "uniqueItems" : true
            }
          ]
        },
        "ids_only" : {
          "$ref" : "#/$defs/boolean_string"
        },
        "phase" : {
          "oneOf" : [
            {
              "$ref" : "common.json#/$defs/device_phase"
            },
            {
              "items" : {
                "$ref" : "common.json#/$defs/device_phase"
              },
              "minItems" : 2,
              "type" : "array",
              "uniqueItems" : true
            }
          ]
        },
        "serials_only" : {
          "$ref" : "#/$defs/boolean_string"
        }
      },
      "type" : "object",
      "unevaluatedProperties" : false
    },
    "BuildRacks" : {
      "additionalProperties" : false,
      "default" : {
        "ids_only" : "0"
      },
      "properties" : {
        "ids_only" : {
          "$ref" : "#/$defs/boolean_string"
        },
        "phase" : {
          "oneOf" : [
            {
              "$ref" : "common.json#/$defs/device_phase"
            },
            {
              "items" : {
                "$ref" : "common.json#/$defs/device_phase"
              },
              "minItems" : 2,
              "type" : "array",
              "uniqueItems" : true
            }
          ]
        }
      },
      "type" : "object"
    },
    "ChangePassword" : {
      "additionalProperties" : false,
      "default" : {
        "clear_tokens" : "login_only"
      },
      "properties" : {
        "clear_tokens" : {
          "enum" : [
            "none",
            "login_only",
            "all"
          ]
        }
      },
      "type" : "object"
    },
    "DeactivateUser" : {
      "$ref" : "#/$defs/ChangePassword"
    },
    "FindDevice" : {
      "additionalProperties" : true,
      "properties" : {
        "phase_earlier_than" : {
          "$comment" : "usually defaults to 'production'",
          "oneOf" : [
            {
              "const" : ""
            },
            {
              "$ref" : "common.json#/$defs/device_phase"
            }
          ]
        }
      },
      "type" : "object"
    },
    "GetBuild" : {
      "$ref" : "#/$defs/WithDeviceRackData",
      "unevaluatedProperties" : false
    },
    "GetBuilds" : {
      "additionalProperties" : false,
      "not" : {
        "properties" : {
          "completed" : {
            "const" : 1
          },
          "started" : {
            "const" : 0
          }
        },
        "required" : [
          "started",
          "completed"
        ]
      },
      "properties" : {
        "completed" : {
          "$ref" : "#/$defs/boolean_string"
        },
        "started" : {
          "$ref" : "#/$defs/boolean_string"
        }
      },
      "type" : "object"
    },
    "GetDeviceByAttribute" : {
      "additionalProperties" : {
        "type" : "string"
      },
      "maxProperties" : 1,
      "minProperties" : 1,
      "properties" : {
        "hostname" : {
          "type" : "string"
        },
        "ipaddr" : {
          "$ref" : "common.json#/$defs/ipaddr"
        },
        "link" : {
          "format" : "uri",
          "type" : "string"
        },
        "mac" : {
          "$ref" : "common.json#/$defs/macaddr"
        }
      },
      "propertyNames" : {
        "$ref" : "common.json#/$defs/setting_key"
      },
      "type" : "object"
    },
    "GetValidationState" : {
      "$ref" : "#/$defs/FindDevice",
      "properties" : {
        "status" : {
          "oneOf" : [
            {
              "$ref" : "common.json#/$defs/validation_status"
            },
            {
              "items" : {
                "$ref" : "common.json#/$defs/validation_status"
              },
              "minItems" : 2,
              "type" : "array",
              "uniqueItems" : true
            }
          ]
        }
      },
      "type" : "object",
      "unevaluatedProperties" : false
    },
    "HardwareProductSpecification" : {
      "additionalProperties" : false,
      "properties" : {
        "path" : {
          "format" : "json-pointer",
          "type" : "string"
        }
      },
      "required" : [
        "path"
      ],
      "type" : "object"
    },
    "NotifyUsers" : {
      "$comment" : "used for operations where email can optionally be sent",
      "additionalProperties" : false,
      "default" : {
        "send_mail" : "1"
      },
      "properties" : {
        "send_mail" : {
          "$ref" : "#/$defs/boolean_string"
        }
      },
      "type" : "object"
    },
    "Null" : {
      "additionalProperties" : false,
      "type" : "object"
    },
    "ProcessDeviceReport" : {
      "additionalProperties" : false,
      "default" : {
        "no_save_db" : "0"
      },
      "properties" : {
        "no_save_db" : {
          "$ref" : "#/$defs/boolean_string"
        }
      },
      "type" : "object"
    },
    "ResetUserPassword" : {
      "additionalProperties" : false,
      "default" : {
        "clear_tokens" : "login_only",
        "send_mail" : "1"
      },
      "properties" : {
        "clear_tokens" : {
          "enum" : [
            "none",
            "login_only",
            "all"
          ]
        },
        "send_mail" : {
          "$ref" : "#/$defs/boolean_string"
        }
      },
      "type" : "object"
    },
    "RevokeUserTokens" : {
      "additionalProperties" : false,
      "default" : {
        "api_only" : "0",
        "login_only" : "0",
        "send_mail" : "1"
      },
      "not" : {
        "properties" : {
          "api_only" : {
            "const" : "1"
          },
          "login_only" : {
            "const" : "1"
          }
        },
        "required" : [
          "login_only",
          "api_only"
        ],
        "type" : "object"
      },
      "properties" : {
        "api_only" : {
          "$ref" : "#/$defs/boolean_string"
        },
        "login_only" : {
          "$ref" : "#/$defs/boolean_string"
        },
        "send_mail" : {
          "$ref" : "#/$defs/boolean_string"
        }
      },
      "type" : "object"
    },
    "SetPhase" : {
      "additionalProperties" : false,
      "default" : {
        "rack_only" : "0"
      },
      "properties" : {
        "rack_only" : {
          "$ref" : "#/$defs/boolean_string"
        }
      },
      "type" : "object"
    },
    "WithDeviceRackData" : {
      "additionalProperties" : true,
      "properties" : {
        "with_device_health" : {
          "$ref" : "#/$defs/boolean_string_or_flag"
        },
        "with_device_phases" : {
          "$ref" : "#/$defs/boolean_string_or_flag"
        },
        "with_rack_phases" : {
          "$ref" : "#/$defs/boolean_string_or_flag"
        }
      },
      "type" : "object"
    },
    "boolean_string" : {
      "enum" : [
        "0",
        "1"
      ],
      "type" : "string"
    },
    "boolean_string_or_flag" : {
      "$comment" : "\"?foo\" and \"?foo=1\" are true; \"?foo=0\" is false",
      "enum" : [
        "",
        "0",
        "1"
      ],
      "type" : "string"
    },
    "non_negative_integer_string" : {
      "$comment" : "see common.json#/$defs/non_negative_integer",
      "pattern" : "^[0-9]+$",
      "type" : "string"
    }
  },
  "$schema" : "https://json-schema.org/draft/2019-09/schema"
}
